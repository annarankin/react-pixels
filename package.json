{
  "name": "react-pixels",
  "productName": "Reacty Pixels",
  "version": "1.0.0",
  "description": "Playing around with react and canvas in an attempt to create a pixel art editor",
  "main": "main.js",
  "scripts": {
    "start": "npm run build; electron main.js",
    "watch": "webpack --watch | electron main.js",
    "build": "cp -r ./src/images ./app; webpack",
    "postinstall": "install-app-deps",
    "pack": "build --dir",
    "dist": "build"
  },
  "build": {
    "appId": "anna.has.an.id",
    "category": "fun",
    "win": {
      "iconUrl": "https://i.imgur.com/4G51NSI.png"
    }
  },
  "target": "electron",
  "author": "Anna Rankin",
  "license": "ISC",
  "dependencies": {
    "lodash": "^4.13.1",
    "react": "^15.2.1",
    "react-css-modules": "^3.7.7",
    "react-dom": "^15.2.1",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "redux-undo": "^0.6.1"
  },
  "devDependencies": {
    "babel-core": "^6.10.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-export-extensions": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "brfs": "^1.4.3",
    "css-loader": "^0.23.1",
    "electron": "^1.3.4",
    "electron-builder": "^6.3.2",
    "electron-prebuilt": "^1.3.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-webpack-plugin": "^2.22.0",
    "style-loader": "^0.13.1",
    "transform-loader": "^0.2.3",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  }
}
